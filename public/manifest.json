{
  "manifest_version": 2,
  "name": "Clickbait Checker",
  "version": "1.0",
  "description": "Check if the content matches the title for clickbait",
  "browser_action": {
    "default_popup": "index.html"
  },
  "default_popup": {
    "width": 400,
    "height": 600
  },
  "permissions": [
    "activeTab"
  ]
}
